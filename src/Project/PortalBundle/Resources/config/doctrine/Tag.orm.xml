<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Project\PortalBundle\Entity\Tag" table="Tag">
    <unique-constraints>
      <unique-constraint name="tag_name_UNIQUE" columns="tag_name"/>
    </unique-constraints>
    <id name="tagId" type="integer" column="tag_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tagName" type="string" column="tag_name" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
